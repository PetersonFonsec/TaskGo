    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.12",
    "@types/node": "^20.17.19",
    "jasmine-core": "~5.12.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
